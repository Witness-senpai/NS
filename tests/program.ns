!!Factorial from 20
factorial = 1;
i = 2;
end = 20; 


while(i <= end){
    factorial *= i;
    i++;
}

print(end . "! = " . factorial);
print("======================");

!!Power of two (from -10 to 32)
i = -10;
while(i <= 32){
    print("2 ^ " . i . " = ". 2 ** i);
    i++;
}
print("======================");

!!100 number of Fibonacci
fib0 = 0;
fib1 = 1;
end = 100;
i = 1;

while (i <= end){
    if(i // 2 == 0){
        fib0 += fib1;
    } else {
        fib1 += fib0;
    }
    i++;
}

if (fib0 > fib1){
    print(end . " Fibonacci number is " . fib0);
} else {
    print(end . " Fibonacci number is " . fib1);
}

end.